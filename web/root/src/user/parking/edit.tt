<fieldset>

    [%input_text('name','Nome', value => vehicle_parking_obj.name)|none%]
    [%input_text('entry_time','Hora de entrada', value => vehicle_parking_obj.entry_time)|none%]
    [%input_text('departure_time','Hora da saida', value => vehicle_parking_obj.departure_time)|none%]

    [%input_text('address','EndereÃ§o', value => vehicle_parking_obj.address)|none%]


