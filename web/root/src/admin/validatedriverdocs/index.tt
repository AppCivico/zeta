[%-CALL assets.include("js/admin.js")%]
<h2>Lista de documentos</h2>

<table class="table table-striped">

<thead>
<tr>
    <th>Documento</th>
    <th>Validado em</th>
    <th>Validado por</th>
    <th>Opções</th>
</tr>
</thead>
<tbody>
    [%FOREACH cs IN documents%]
    <tr>
        <th>[%cs.class_name%]</th>
        <td>
        [%IF cs.validated_at%]
            [%format_date_to_human(cs.validated_at)%]
        [%ELSE%]
            --
        [%END%]
        </td>
        <th>
        [%IF cs.validated_by%]
            [%cs.validated_by%]
        [%ELSE%]
            --
        [%END%]
        </th>
        <td><a href="/patch/to/cuspir/download/fuck">Download</a></td>
        <td><a href="javascript:;"  class="icon-ok-sign approve_docs" id="accept" value="[%cs.id%]" title="Aprovar documento"></a></td>
        <td><a href="javascript:;" data-confirm="Deseja realmente não aprovar?" id="deny" value="[%cs.id%]" class="icon-remove-sign approve_docs" title="Negar documento"></a></td>
    </tr>
    [%END%]
</tbody>
</table>

<a class="btn" href="/admin/campaign/new">Adicionar uma nova campanha</a>